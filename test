 

data() {
  return {
     // your other data...
     pdfSrc: '',
     showModal: false,
     // rest of your data properties
  };
},
methods:{
  // your other methods...
  showFile(pdfBase64) {
     this.pdfSrc = "data:application/pdf;base64," + pdfBase64;
     this.showModal = true;
  },
  // your other methods...
}

<div v-if="showModal" class="modal">
  <span class="close" @click="showModal=false">&times;</span>
  <embed :src="pdfSrc" type="application/pdf" width="100%" height="600px"/>
</div>

-----------------
<td>
   <button @click="showFile(item.p_liber_file_path)">Show File</button>
</td>
----------------
.modal {
  display: block;
  position: fixed;
  z-index: 1;
  padding-top: 100px;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  overflow: auto;
  background-color: rgba(0,0,0,0.4);
}

.modal-content {
  position: relative;
  margin: auto;
  padding: 0;
  width: 90%;
}

.close {
  position: absolute;
  right: 25px;
  top: 0;
  color: #000;
  font-size: 35px;
  font-weight: bold;
}

.close:hover,
.close:focus {
  color: #999;
  text-decoration: none;
  cursor: pointer;
}
